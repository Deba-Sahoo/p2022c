  <script>
   function startSlider(sliderId) {
  const slider = document.getElementById(sliderId);
  const slides = slider.querySelectorAll('.slide');
  let currentIndex = 0;
  let interval;

  function moveSlider() {
    currentIndex++;
    if (currentIndex >= slides.length) {
      currentIndex = 0;
    }
    slider.style.transform = `translateX(${-currentIndex * slides[0].offsetWidth}px)`;
  }

  slider.addEventListener('mouseover', () => {
    clearInterval(interval);
    slides[currentIndex].style.transform = 'scale(1.1)';
  });

  slider.addEventListener('mouseout', () => {
    interval = setInterval(moveSlider, 2000);
    slides[currentIndex].style.transform = 'scale(1)';
  });

  interval = setInterval(moveSlider, 2000);
}

function moveSlider(sliderId, direction) {
  const slider = document.getElementById(sliderId);
  const slides = slider.querySelectorAll('.slide');
  const currentIndex = Math.round(-slider.style.transform.replace('translateX(', '').replace('px)', '') / slides[0].offsetWidth) || 0;
  let newIndex = currentIndex + direction;
  if (newIndex < 0) newIndex = slides.length - 1;
  if (newIndex >= slides.length) newIndex = 0;
  slider.style.transform = `translateX(${-newIndex * slides[0].offsetWidth}px)`;
}

