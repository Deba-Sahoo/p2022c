}

.company-slider {
  margin: 40px 0;
}

h2 {
  text-align: center;
  font-family: 'Comic Neue', cursive;
}

.slider-container {
  position: relative;
  width: 100%;
  overflow: hidden;
}

.slider {
  display: flex;
  transition: transform 0.5s ease;
}

.slide {
  flex: 0 0 300px;
  height: 350px;
  margin-left: 40px;
  margin: 0 30px;
  transition: transform 0.3s ease;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
  border-radius: 10px;
  background-color: #fff;
  overflow: hidden;
}

.slide img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  border-radius: 10px;
}

.slide:hover {
  transform: scale(1.1);
}

.arrow {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background-color: rgba(0, 0, 0, 0.5);
  color: white;
  font-size: 30px;
  cursor: pointer;
  padding: 10px;
  z-index: 1;
  border-radius: 50%;
  user-select: none;
}

.arrow.prev {
  left: 10px;
}

.arrow.next {
  right: 10px;
}

@media (max-width: 1020px) {
  .slide {
    flex: 0 0 250px;
    height: 350px;
  }

  .arrow {
    font-size: 24px;
    padding: 8px;
  }
}

@media (max-width: 700px) {
  .slide {
    flex: 0 0 200px;
    height: 300px;
  }

  .arrow {
    font-size: 20px;
    padding: 6px;
  }
}
