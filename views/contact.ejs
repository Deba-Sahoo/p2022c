      <h2>Contact <span> Us </span></h2>

      <section class="contact" id="contact">
        <div class="container">
          <div class="row">
            <div class="col-md-5 side">
              <div class="content">
                <!-- Info-1 -->
                <div class="info">
                  <i class="fas fa-mobile-alt lr"></i>
                  <h4 class="d-inline-block">
                    PHONE :
                    <br />
                    <span>+91 9438688816</span>
                  </h4>
                </div>
                <!-- Info-2 -->
                <div class="info">
                  <i class="far fa-envelope"></i>
                  <h4 class="d-inline-block">
                    EMAIL :
                    <br />
                    <span>placement@cet.edu.in</span>
                  </h4>
                </div>
                <!-- Info-3 -->
                <div class="info">
                  <i class="fas fa-map-marker-alt mob lr1"></i>
                  <h4 class="d-inline-block mob2">
                    ADDRESS :<br />
                    <span
                      >College of Engineering & Technology, Ghatikia,
                      Bhubaneswar.</span
                    >
                  </h4>
                </div>
              </div>
            </div>

            <div class="col-md-7">
              <form action="" method="post">
                <div class="row">
                  <div class="col-sm-6">
                    <input
                      type="text"
                      class="form-control"
                      placeholder="Name"
                    />
                  </div>
                  <div class="col-sm-6">
                    <input
                      type="email"
                      class="form-control"
                      placeholder="Email"
                    />
                  </div>
                  <div class="col-sm-12">
                    <input
                      type="text"
                      class="form-control"
                      placeholder="Subject"
                    />
                  </div>
                </div>
                <div class="form-group">
                  <textarea
                    class="form-control"
                    rows="5"
                    id="comment"
                    placeholder="Message"
                  ></textarea>
                </div>
                <button class="btn btn-block" type="submit">Send Now!</button>
              </form>
            </div>
          </div>
        </div>
