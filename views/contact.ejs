    <title>Contact Us</title>
    <style>
      body {
        background-color: #fcfcfc;
      }
      legend {
        margin-top: 30px;
      }
      .one {
        font-size: 1.4em;
        font-weight: bold;
      }
      .two {
        font-size: 1.1em;
      }
      iframe {
        width: 100%;
        height: 40vh;
        margin: 3em 0 1em;
      }
      .card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
        max-width: 300px;
        margin: 1em;
        text-align: center;
      }

      .title {
        color: grey;
        font-size: 18px;
      }

      .space {
        margin: 0 7em;
      }

      .contact {
        padding: 30px 0 60px 0;
      }

      #gmap_container {
        margin: 2em 0;
      }

      h2 {
        font-size: 30px;
        font-weight: 700;
        margin: 0;
        padding: 0;
        text-align: center;
      }

      h2 span {
        color: #007ccb;
      }

      .contact .heading p {
        font-size: 15px;
        font-weight: 400;
        line-height: 1.7;
        color: #999999;
        margin: 20px 0 60px;
        padding: 0;
      }

      .contact .form-control {
        padding: 25px;
        font-size: 13px;
        margin-bottom: 10px;
        background: #eeeeee;
        border: 0;
        border-radius: 10px;
      }

      .contact button.btn {
        padding: 10px;
        border-radius: 10px;
        font-size: 15px;
        background: #007ccb;
        color: #ffffff;
      }

      .contact .title h3 {
        font-size: 18px;
        font-weight: 600;
      }

      .contact .title p {
        font-size: 14px;
        font-weight: 400;
        color: #999;
        line-height: 1.6;
        margin: 0 0 40px;
      }

      .contact .content .info {
        margin-top: 3em;
      }
      .contact .content .info i {
        font-size: 30px;
        padding: 0;
        margin: 0;
        color: #02434b;
        margin-right: 16px;
        text-align: center;
        width: 20px;
      }
      .contact .content .info .lr,
      .contact .content .info .lr1 {
        margin: 0 13px 0 3px;
      }
      .contact .content .info h4 {
        font-size: 13px;
        line-height: 1.4;
      }

      .contact .content .info h4 span {
        font-size: 13px;
        font-weight: 300;
        color: #999999;
      }

      @media (max-width: 800px) {
        .space {
          margin: 0 auto;
        }
        .mob {
          width: 20%;
        }
        .mob2 {
          width: 80%;
        }
        .contact .content .info {
          margin: 2rem 0;
        }
        .contact .content .info i {
          float: left;
          margin-right: 30px;
        }
        .contact .content .info .lr {
          margin: 0 27px 0 3px;
        }
        .contact .content .info .lr1 {
          margin: 10px 27px 0 3px;
        }
        .side {
          margin-left: 2em;
        }
      }
    </style>
