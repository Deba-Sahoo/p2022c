      }
      
      iframe {
        width: 300px;
        height: 300px;
      }
      .footer-item{
        display: grid;
        justify-content: center;
        margin-left: 20px;
        margin-right: auto;
        margin-top: 0;
      }
      .left {
        margin-left: 24px;
      }
    }
