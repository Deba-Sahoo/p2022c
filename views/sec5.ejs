    <style>
      center {
        cursor: pointer;
      }
        :root {
        --gutter: 1.5rem;
        --bg-color: #edf2f7;
        --speed: 0.3s;
        }

        .project-list {
        display: grid;
        grid-gap: calc(var(--gutter) * 1.5 + 1vw);
        grid-template-columns: 1rem;
        grid-template-rows: minmax(10rem, 1fr);
        grid-auto-flow: column;
        grid-auto-columns: 320px;
        overflow-x: scroll;
        scroll-snap-type: x proximity;
        scroll-snap-points-x: repeat(100%);
        scroll-snap-type: mandatory;
        scroll-snap-destination: 100% 0%;
        padding-bottom: calc(.75 * var(--gutter));
        margin-bottom: calc(-.25 * var(--gutter));
        -webkit-overflow-scrolling: touch;
        }
        .project-list::-webkit-scrollbar {
        display: none;
        }

        h1{
            font-size: 2.5em;
            margin-bottom: .5rem;
            text-align: center;
        }

        @media (min-width: 800px) {
        .project-list:hover .item {
            opacity: 0.6;
            filter: blur(2px);
        }
        }

        @media screen and (min-width: 640px) {
        .mask-right {
            mask: linear-gradient(-90deg, rgba(0, 0, 0, 0) 3%, black 20%);
            -webkit-mask: linear-gradient(-90deg, rgba(0, 0, 0, 0) 3%, black 20%);
        }

        footer ul.f-address {
          padding: inherit;
        }
        }

        .flag {
        transition: all var(--speed) ease;
        transform: translate3d(6rem, -6rem, 0) rotate(45deg);
        }

        .project-list:before,
        .project-list:after {
        content: '';
        width: 10px;
        }

        .item {
        scroll-snap-align: center;
        padding: calc(var(--gutter) / 2 * 1.5);
        transition: all var(--speed);
        }
        .item:hover {
        opacity: 1 !important;
        filter: blur(0) !important;
        }
        .item:hover .flag {
        transform: translate3d(4rem, -4rem, 0) rotate(45deg);
        }

        .item:nth-last-child {
        margin-right: 5rem;
        }

        .lozad {
        transition: opacity var(--speed);
        opacity: 1;
        }

        /* .lozad.loaded {
        opacity: 1;
        } */

        .toggled {
        transition: opacity var(--speed);
        transition-delay: var(--speed);
        opacity: 1;
        }

        .no-scrollbar {
        scrollbar-width: none;
        margin-bottom: 0;
        padding-bottom: 0;
        }

        .no-scrollbar::-webkit-scrollbar {
        display: none;
        }

        .is-active {
        display: block;
        }

        .icon, .toggle-me, .nav-item {
        transition: all var(--speed) ease;
        }

        .is-toggled {
        color: black;
        }
        .is-toggled .icon-default {
        display: none;
        }
        .is-toggled .icon-active {
        display: block;
        }

        /* .nav-items:nth-child(1) a {
